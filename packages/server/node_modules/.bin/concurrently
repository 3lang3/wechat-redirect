#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/3lang/Workspace/github/wxopen/node_modules/.pnpm/concurrently@7.2.1/node_modules/concurrently/dist/bin/node_modules:/Users/3lang/Workspace/github/wxopen/node_modules/.pnpm/concurrently@7.2.1/node_modules/concurrently/dist/node_modules:/Users/3lang/Workspace/github/wxopen/node_modules/.pnpm/concurrently@7.2.1/node_modules/concurrently/node_modules:/Users/3lang/Workspace/github/wxopen/node_modules/.pnpm/concurrently@7.2.1/node_modules:/Users/3lang/Workspace/github/wxopen/node_modules/.pnpm/node_modules:/Users/3lang/Workspace/github/wxopen/node_modules:/Users/3lang/Workspace/github/node_modules:/Users/3lang/Workspace/node_modules:/Users/3lang/node_modules:/Users/node_modules:/node_modules:/Users/3lang/Workspace/github/wxopen/packages/server/node_modules:/Users/3lang/Workspace/github/wxopen/packages/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/3lang/Workspace/github/wxopen/node_modules/.pnpm/concurrently@7.2.1/node_modules/concurrently/dist/bin/node_modules:/Users/3lang/Workspace/github/wxopen/node_modules/.pnpm/concurrently@7.2.1/node_modules/concurrently/dist/node_modules:/Users/3lang/Workspace/github/wxopen/node_modules/.pnpm/concurrently@7.2.1/node_modules/concurrently/node_modules:/Users/3lang/Workspace/github/wxopen/node_modules/.pnpm/concurrently@7.2.1/node_modules:/Users/3lang/Workspace/github/wxopen/node_modules/.pnpm/node_modules:/Users/3lang/Workspace/github/wxopen/node_modules:/Users/3lang/Workspace/github/node_modules:/Users/3lang/Workspace/node_modules:/Users/3lang/node_modules:/Users/node_modules:/node_modules:/Users/3lang/Workspace/github/wxopen/packages/server/node_modules:/Users/3lang/Workspace/github/wxopen/packages/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../node_modules/.pnpm/concurrently@7.2.1/node_modules/concurrently/dist/bin/concurrently.js" "$@"
else
  exec node  "$basedir/../../../../node_modules/.pnpm/concurrently@7.2.1/node_modules/concurrently/dist/bin/concurrently.js" "$@"
fi
